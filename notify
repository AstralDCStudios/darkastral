

local Loader = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextLabel = Instance.new("TextLabel")
local UIScale = Instance.new("UIScale")



Loader.Name = "Loader"
Loader.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Loader.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = Loader
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.BackgroundTransparency = 0.500
Frame.Position = UDim2.new(0, 0, -0.300000012, 0)
Frame.Size = UDim2.new(1, 0, -0.055555556, 100)

UICorner.Parent = Frame

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Size = UDim2.new(1, 0, 1, 0)
TextLabel.Font = Enum.Font.GothamMedium
TextLabel.Text = "Press V To Open Dark Astral"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 23.000
TextLabel.TextTransparency = 0.720

UIScale.Parent = Frame

-- Scripts:

local function EVYTI_fake_script() -- Frame.Open 
	local script = Instance.new('LocalScript', Frame)

	local TweenService = game:GetService("TweenService")
	local Gui = script.Parent
	
	local info = TweenInfo.new(
		1, --Length
		Enum.EasingStyle.Bounce, --Type of Tween (easing style)
		Enum.EasingDirection.Out, --Easing Direction
		0, --Ammount of times it repeats
		false, --Reverse? (go back up)
		0 --Delay
	)
	
	local Goals =
	{
			Position = UDim2.new(0, 0,0.479, 0); 
	}
	
	local ButtonTween = TweenService:Create(Gui, info, Goals)	
	
	wait(2)
	ButtonTween:Play()
	
end
coroutine.wrap(EVYTI_fake_script)()
local function TNWYVW_fake_script() -- Frame.Closes 
	local script = Instance.new('LocalScript', Frame)

	local TweenService = game:GetService("TweenService")
	local Gui = script.Parent
	
	local info = TweenInfo.new(
		1, --Length
		Enum.EasingStyle.Bounce, --Type of Tween (easing style)
		Enum.EasingDirection.Out, --Easing Direction
		0, --Ammount of times it repeats
		false, --Reverse? (go back up)
		0 --Delay
	)
	
	local Goals =
	{
			Position = UDim2.new(0, 0,-0.3, 0); 
	}
	
	local ButtonTween = TweenService:Create(Gui, info, Goals)	
	
	wait(6)
	ButtonTween:Play()
	wait(1)
	Loader:Destroy()
end
coroutine.wrap(TNWYVW_fake_script)()
